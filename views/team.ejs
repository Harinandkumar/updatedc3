  <!-- ðŸ‘¨â€ðŸ« FACULTY MENTOR SECTION -->
  <section class="section" id="faculty-mentors">
      <div class="section-title">
          <h2>Our Faculty Mentors</h2>
      </div>
      <div class="faculty-container">
          <div class="faculty-grid">
              <div class="faculty-card">
                  <div class="faculty-img-box">
                      <img src="/assets/img/rajesh sir.jpg" alt="Faculty Mentor" class="faculty-img">
                  </div>
                  <h3>Rajesh Kumar</h3>
                  <p class="faculty-dept">CSE Department</p>
                  <div class="faculty-links">
                      <a href="#"><i class="fab fa-github"></i></a>
                      <a href="#"><i class="fab fa-linkedin"></i></a>
                  </div>
              </div>
              <div class="faculty-card">
                  <div class="faculty-img-box">
                      <img src="/assets/img/shafaque mam.jpg" alt="Faculty Mentor" class="faculty-img">
                  </div>
                  <h3>Shafaque Aziz</h3>
                  <p class="faculty-dept">CSE Department</p>
                  <div class="faculty-links">
                      <a href="#"><i class="fab fa-github"></i></a>
                      <a href="#"><i class="fab fa-linkedin"></i></a>
                  </div>
              </div>
          </div>
      </div>
  </section>


  <!-- ðŸŒŸ Core Members -->
  <section class="team-section">
    <h2>Core Team</h2>
    <div class="team-grid">
      <% coreMembers.forEach(member => { %>
        <div class="team-card">
          <div class="team-img"><img src="<%= member.img %>" alt="<%= member.name %>"></div>
          <h3><%= member.name %></h3>
          <p class="role"><%= member.role %></p>
          <div class="social-links">
            <a href="<%= member.github %>" target="_blank"><i class="fab fa-github"></i></a>
            <a href="<%= member.linkedin %>" target="_blank"><i class="fab fa-linkedin"></i></a>
          </div>
        </div>
      <% }) %>
    </div>
  </section>

  <!-- ðŸ‘©â€ðŸ’¼ Coordinators -->
  <section class="team-section">
    <h2>Our Coordinators</h2>
    <div class="team-grid faculty-grid">
      <% coordinators.forEach(member => { %>
        <div class="faculty-card ">
          <div class="team-img"><img src="<%= member.img %>" alt="<%= member.name %>"></div>
          <h3><%= member.name %></h3>
          <p class="role"><%= member.role %></p>
          <div class="social-links">
            <a href="<%= member.github %>" target="_blank"><i class="fab fa-github"></i></a>
            <a href="<%= member.linkedin %>" target="_blank"><i class="fab fa-linkedin"></i></a>
          </div>
        </div>
      <% }) %>
