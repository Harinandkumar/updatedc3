  <!-- üí´ HERO SECTION -->
  <!-- üí´ HERO SECTION -->
<!-- üí´ HERO SECTION -->
<section id="home" class="hero-section">
  <!-- Matrix Canvas Background -->
  <canvas id="matrixCanvas"></canvas>

  <!-- üåà Floating Neon Circles -->
  <div class="floating-circles">
    <div class="circle circle1"></div>
    <div class="circle circle2"></div>
    <div class="circle circle3"></div>
  </div>

  <!-- Semi-transparent overlay -->
  <div class="hero-overlay"></div>

  <!-- üåü Hero Content -->
  <div class="hero-content">
    <h1 class="hero-title" data-aos="fade-up">
      Creative <span>Coding</span> Community
    </h1>
    <p class="hero-subtitle" data-aos="fade-up" data-aos-delay="100">
      Join our vibrant community of coders, creators, and innovators. Collaborate, learn, and build amazing projects with like-minded individuals.
    </p>
    <div class="hero-buttons" data-aos="fade-up" data-aos-delay="200">
      <a href="#events" class="btn-primary"><i class="fas fa-calendar"></i> Explore Events</a>
      <a href="/team" class="btn-outline"><i class="fas fa-users"></i> Meet the Team</a>
    </div>
  </div>
</section>

  <!-- ‚ö° EVENTS SECTION -->
  <section id="events" class="section events-section">
    <div class="section-header">
      <h2>Upcoming Events</h2>
      <p>Explore our latest hackathons, workshops, and coding challenges.</p>
    </div>

    <div class="events-grid" id="eventsGrid">
      <% if (events && events.length > 0) { %>
        <% events.forEach(e => { %>
          <div class="event-card" data-aos="fade-up">
            <div class="event-image-box">
              <img src="<%= e.imagelink || '/assets/img/default-event.jpg' %>" alt="<%= e.name %>" class="event-image" />
            </div>
            <div class="event-info">
              <h3><%= e.name %></h3>
              <p><%= e.description || 'No description available.' %></p>
              <div class="event-meta">
                <span><i class="far fa-calendar-alt"></i> 
                  <%= e.date ? new Date(e.date).toLocaleDateString('en-US', { year: 'numeric', month: 'short', day: 'numeric' }) : 'TBA' %>
                </span>
                <span><i class="fas fa-map-marker-alt"></i> <%= e.location || 'TBA' %></span>
                <span><i class="fas fa-trophy"></i> <%= e.prize || 'Exciting Rewards' %></span>
              </div>
              <a href="<%= e.link && e.link.trim() !== '' ? e.link : 'https://forms.gle/XYZ123' %>" 
                 target="_blank" class="event-btn">Register Now</a>
            </div>
          </div>
        <% }) %>
      <% } else { %>
        <p class="no-events">No upcoming events yet üö´</p>
      <% } %>
    </div>
  </section>
<!-- üëë CODER OF THE MONTH SECTION -->
<section class="coder-section" id="coderOfMonth">
  <h2>üî• C3 LOGIC LEAGUE ‚Äì Result üî•</h2>
  <h2 class="coder-title">üëë Coders of the Month</h2>

  <div class="coder-row">
    <% if (coders && coders.length > 0) { %>
      <% coders.forEach(coder => { %>
        <div class="coder-card coder-month">
          <div class="coder-img-wrapper">
            <img class="coder-img" src="<%= coder.image %>" alt="<%= coder.name %>" />
          </div>
          <h3><%= coder.name %></h3>
          <p>Roll No: <%= coder.roll %></p>
          <p>Achievement: <%= coder.achievement %></p>
        </div>
      <% }) %>
    <% } else { %>
      <p class="no-events">No coders of the month yet üöÄ</p>
    <% } %>
  </div>
</section>

  <!-- üì¢ NOTICES SECTION -->
  <section id="notices" class="section notices-section">
    <div class="section-header">
      <h2>Latest Notices</h2>
      <p>Stay updated with the latest announcements and opportunities üí¨</p>
    </div>

    <div class="notices-container" id="noticesContainer">
      <% if (notices && notices.length > 0) { %>
        <% notices.forEach(n => { %>
          <div class="notice-item" data-aos="fade-up">
            <h4><%= n.title %></h4>
            <p><%= n.message %></p>
            <small><i class="far fa-clock"></i>
              <%= new Date(n.date).toLocaleDateString('en-US', { year: 'numeric', month: 'short', day: 'numeric' }) %>
            </small>
          </div>
        <% }) %>
      <% } else { %>
        <p>No new notices at the moment üì≠</p>
      <% } %>
    </div>
  </section>

